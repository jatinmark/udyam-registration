{
  "buildCommand": "npx prisma generate && npm run build",
  "framework": "nextjs",
  "installCommand": "npm install",
  "env": {
    "DATABASE_URL": "@database_url",
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "DATABASE_URL": "@database_url",
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  }
}